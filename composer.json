{
  "name": "sentinel-service/laravel-remote-config",
  "description": "Remote configuration management for Laravel applications",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Sentinel Team",
      "email": "team@sentinel.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "SentinelService\\RemoteConfig\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "SentinelService\\RemoteConfig\\RemoteConfigServiceProvider"
      ],
      "aliases": {
        "RemoteConfig": "SentinelService\\RemoteConfig\\Facades\\RemoteConfig"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}